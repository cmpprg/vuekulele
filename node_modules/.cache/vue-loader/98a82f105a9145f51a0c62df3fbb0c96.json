tring: (startNote, collectionOfNotes) => {\n        let copyNotes = [...collectionOfNotes]\n        while (copyNotes[0] !== startNote) {\n          copyNotes.unshift(copyNotes.pop())\n        }\n        return copyNotes.slice(0, 12)\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .string {\n    display: block;\n    padding-bottom: 40px;\n    height: 20px;\n    width: 100%;\n\n  }\n</style>\n"]}]}